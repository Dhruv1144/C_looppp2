#include<stdio.h>
#include<conio.h>

int main()

{
	int i,j;
	
	char input,alphabet='A';
	printf("enter an upper case character you want to printin the last row::");
	scanf("%c",&input);
	
	for(i=1;i<=(input-'A'+1);++i);
	{
		for (j=1;j<=i;++j)
		{
			printf("%c",alphabet);
		
		}
		++alphabet;
		printf("\n");
		}	
		getch();
}
